{
    "formId": "survey_001",
    "title": "Customer Feedback Form",
    "description": "Help us improve by answering these questions.",
    "currentPage": 1,
    "totalPages": 3,
    "pages": [
        {
        "pageId": "page_1",
        "title": "Personal Information",
        "questions": [
            {
            "questionId": "q1",
            "type": "text",
            "label": "What is your full name?",
            "required": true,
            "validation": {
                "pattern": "^[a-zA-Z\\s]+$",
                "errorMessage": "Only letters and spaces are allowed."
            }
            },
            {
            "questionId": "q2",
            "type": "select",
            "label": "How did you hear about us?",
            "options": {

                "Google" : "Google",
                "Social Media": "Social Media",
                "Friend" : "Friend",
                "Other" : "Other"
            },
            "required": true
            },
            {
            "questionId": "q3",
            "type": "radio",
            "label": "Are you satisfied with our service?",
            "options": {
                "yes" : "Yes",
                "no" : "No"
            },
            "required": true,
            "branching": {
                "no": "page_3",
                "yes": "page_2"
            }
            }
        ]
        },
        {
        "pageId": "page_2",
        "title": "Improvement Suggestions",
        "conditions": {
            "previousQuestionId": "q3",
            "expectedAnswer": "no"
        },
        "questions": [
            {
            "questionId": "q4",
            "type": "textarea",
            "label": "What can we improve?",
            "required": true
            }
        ]
        },
        {
        "pageId": "page_3",
        "title": "Final Thoughts",
        "questions": [
            {
            "questionId": "q5",
            "type": "checkbox",
            "label": "Which features do you use the most?",
            "options": {
                "Feature A": "Feature A",
                "Feature B": "Feature B",
                "Feature C": "Feature C"
            }
            },
            {
            "questionId": "q6",
            "type": "text",
            "label": "Any additional comments?",
            "required": false
            },
            {
            "questionId": "q7",
            "type": "file",
            "label": "Upload a screenshot or document (optional)",
            "required": false,
            "fileOptions": {
                "maxSizeMB": 5,
                "acceptedFormats": [ "image/png", "image/jpeg", "application/pdf" ]
            }
            }
        ]
        }
    ],
    "submission": {
        "method": "POST",
        "endpoint": "https://api.example.com/submit",
        "headers": {
            "Content-Type": "multipart/form-data"
        }
    }
}